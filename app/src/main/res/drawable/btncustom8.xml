<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- 테두리 내부 색상 설정 -->
            <solid android:color="#FFFFFF" />

            <!-- 테두리 설정: 두께, 색상 -->
            <stroke
                android:width="3dp"
                android:color="#FFFFFF" />
            <corners android:radius="8dp"/>
        </shape>
    </item>
</selector>